generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
} 

model SafeAccount {
  id            String   @id @default(cuid())
  address       String   @unique
  employerAddress  String
  employeeAddress  String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}